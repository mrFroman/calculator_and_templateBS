mplate\"\r\n            :is-text=\"isText\"\r\n        >\r\n        </top-template>\r\n        <technic-template\r\n            v-if=\"template.tops === 0\"\r\n        ></technic-template>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport TopTemplate from \"@/components/email-templater/TopTemplate.vue\";\r\nimport PosterTemplate from \"@/components/email-templater/PosterTemplate.vue\";\r\nimport TechnicTemplate from \"@/components/email-templater/TechnicTemplate.vue\";\r\n\r\nexport default {\r\n    components: {TechnicTemplate, PosterTemplate, TopTemplate},\r\n    props: {\r\n        template: {\r\n            tops: {\r\n                type: Number,\r\n            },\r\n            headPoster: {\r\n                type: Number,\r\n            },\r\n            footerPoster: {\r\n                type: Number,\r\n            }\r\n        }\r\n    },\r\n    computed: {\r\n        isText() {\r\n            return this.template.tops > 1 && this.template.footerPoster === 0 && this.template.headPoster === 0\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.head__poster,\r\n.footer__poster {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  width: 100%;\r\n}\r\n\r\n.root {\r\n  width: 100%;\r\n  min-height: 100%;\r\n  background-color: #fff;\r\n  padding: 15px;\r\n  display: flex;\r\n  justify-content: center;\r\n  flex-wrap: wrap;\r\n}\r\n</style>\r\n"],"mappings":"AAwCA,OAAOA,WAAU,MAAO,8CAA8C;AACtE,OAAOC,cAAa,MAAO,iDAAiD;AAC5E,OAAOC,eAAc,MAAO,kDAAkD;AAE9E,eAAe;EACXC,UAAU,EAAE;IAACD,eAAe;IAAED,cAAc;IAAED;EAAW,CAAC;EAC1DI,KAAK,EAAE;IACHC,QAAQ,EAAE;MACNC,IAAI,EAAE;QACFC,IAAI,EAAEC;MACV,CAAC;MACDC,UAAU,EAAE;QACRF,IAAI,EAAEC;MACV,CAAC;MACDE,YAAY,EAAE;QACVH,IAAI,EAAEC;MACV;IACJ;EACJ,CAAC;EACDG,QAAQ,EAAE;IACNC,MAAMA,CAAA,EAAG;MACL,OAAO,IAAI,CAACP,QAAQ,CAACC,IAAG,GAAI,KAAK,IAAI,CAACD,QAAQ,CAACK,YAAW,KAAM,KAAK,IAAI,CAACL,QAAQ,CAACI,UAAS,KAAM;IACtG;EACJ;AACJ"},"metadata":{},"sourceType"